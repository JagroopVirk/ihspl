---
import Layout from '@/layouts/Layout.astro';

import AboutFull from '@/components/aboutus/AboutFull.astro';

// Importing the SEO
import seoData from '@/data/js/seo/about-us.js';

const { meta, faqSchema, gtm, pageLink, schema } = seoData;
---

<Layout
  seo={{
    title: `${meta.title} - Indivirtus`,
    description: meta.description,
    keywords: meta.keywords,
    canonical: meta.canonicalUrl,
    url: pageLink,
    schema: schema,
    faqSchema: faqSchema,
    ogTags: meta.ogTags,
    twitterCard: meta.twitterCard,
  }}
  gtm={gtm}
>
  <section class="section">
    <div class="maxwidth">
      <h3 class="pageHeading">Who <span class="halfHeading">we are?</span></h3>

      <AboutFull />
    </div>
  </section>
</Layout>
