---
import Layout from '@/layouts/Layout.astro';

// Importing the SEO
import seoData from '@/data/js/seo/blogs.js';

const { meta, faqSchema, gtm, pageLink, schema } = seoData;
---

<Layout
  seo={{
    title: `${meta.title} - Indivirtus`,
    description: meta.description,
    keywords: meta.keywords,
    canonical: meta.canonicalUrl,
    url: pageLink,
    schema: schema,
    faqSchema: faqSchema,
    ogTags: meta.ogTags,
    twitterCard: meta.twitterCard,
  }}
  gtm={gtm}
>
  <section class="section">
    <div class="maxwidth">
      <h3 class="pageHeading">
        Blogs <span class="halfHeading"></span>
      </h3>
    </div>
  </section>
</Layout>
