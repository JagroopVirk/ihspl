---
import Layout from '../layouts/Layout.astro';
import Header from '../components/header/Header.astro';
import Footer from '../components/footer/Footer.astro';
import AboutFull from '../components/aboutus/AboutFull.astro';
import '../styles/global.css';

// Importing the SEO and location schema
import { aboutPageSeo } from '../data/js/seo/siteSeo.js';
import { allServicesSchema } from '../data/js/seo/servicesSchemaSEO.js';
---

<Layout seo={{ ...aboutPageSeo, canonical: aboutPageSeo.url, servicesSchema: allServicesSchema }}>
  <Header />
  <AboutFull />
  <Footer />
</Layout>
